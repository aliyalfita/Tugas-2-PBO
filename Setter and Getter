using System;

namespace SetterGetter
{
    class User
    {
        static void Main(string[] args)
        {
            user Liya = new user();
            Liya.Username = "Liya2002";
            Liya.Password = "sinauC#";

            Console.WriteLine("Username = " + Liya.Username);
            Console.WriteLine("Password = " + Liya.Password);

            Console.ReadKey();
        }
    }
    public class user
    {
        string username;
        string password;
        public string Username
        {
            get { return username; }
            set { username = value; }
        }

        public string Password
        {
            get { return password; }
            set { password = value; }
        }
    }

}
